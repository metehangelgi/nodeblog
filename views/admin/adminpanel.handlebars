<title>Admin Panel</title>
<!--==========================
    INSIDE HERO SECTION Section
============================-->
<section class="page-image page-image-about md-padding">
    <h1 class="text-white text-center">Admin Panel</h1>
</section>

<!--==========================
    ABOUTUS Section
============================-->
<section id="aboutus" class="md-padding">
    <div class="container">
        {{> admin-nav}}
        <div class="clearfix"></div>
        <div class="mb-5"></div>
        <div class="container">
            <a href="/admin/adminpanel/">Home</a>
			<a>>{{kolt.name}}</a>
        </div>
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-8">
                <!-- SideBar -->
                {{> admin-adminbar}}
                <!-- /Sidebar -->
            </div>
            <div class="col-md-4">
                <form action="" method="POST" enctype="multipart/form-data">
                    <div class="row">
                        <div class="col-md-8">
                            <div class="form-group">
                                <input class="form-control" name="name" type="text" placeholder="Category Name * "
                                    required="required" data-validation-required-message="Please enter category name.">
                                <p class="help-block text-danger"></p>
                            </div>
                        </div>
                        <div class="clearfix"></div>
                        <div class="col-lg-12 text-left">
                            <div id="success"></div>
                            <button class="main-btn" style="margin-left: 0;" type="submit">Add Category</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>